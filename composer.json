{
    "name": "sitepoint/nitpick-test",
    "type": "library",
    "description": "nitpick-test package for a SitePoint tutorial",
    "keywords": [
        "sitepoint",
        "nitpick-test"
    ],
    "homepage": "https://github.com/sitepoint/nitpick-test",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Skvorc",
            "email": "bruno@skvorc.me",
            "homepage": "http://bitfalls.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "sitepoint\\nitpick-test\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "sitepoint\\nitpick-test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
